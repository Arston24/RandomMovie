<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" android:id="@+id/profile"
    app:startDestination="@id/savedFragment">

    <fragment
        android:id="@+id/savedFragment"
        android:name="com.victorsysuev.randommovie.ui.profile.ProfileFragment"
        android:label="SavedFragment" >
        <action
            android:id="@+id/action_savedFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
    </fragment>
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.victorsysuev.randommovie.ui.auth.LoginFragment"
        android:label="LoginFragment" />
</navigation>